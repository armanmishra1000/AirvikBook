    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "prisma": "^5.22.0",
    "winston": "^3.11.0"
