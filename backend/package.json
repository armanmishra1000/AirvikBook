{
  "name": "airvikbook-backend",
  "version": "1.0.0",
  "description": "Backend API for AirVikBook Hotel Management System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "dev:windows": "set NODE_ENV=development && nodemon src/server.ts",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:windows": "set NODE_ENV=production && node dist/server.js",
    "build": "tsc",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:backend": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:dev": "cross-env NODE_ENV=development prisma db push",
    "db:push:prod": "cross-env NODE_ENV=production prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:dev": "cross-env NODE_ENV=development prisma migrate deploy",
    "db:migrate:prod": "cross-env NODE_ENV=production prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:studio:dev": "cross-env NODE_ENV=development prisma studio",
    "db:studio:prod": "cross-env NODE_ENV=production prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "supabase:init": "cross-env NODE_ENV=development ts-node scripts/supabase-db-manager.ts init",
    "supabase:stats": "cross-env NODE_ENV=development ts-node scripts/supabase-db-manager.ts stats",
    "supabase:reset": "cross-env NODE_ENV=development ts-node scripts/supabase-db-manager.ts reset",
    "supabase:info": "cross-env NODE_ENV=development ts-node scripts/supabase-db-manager.ts info",
    "supabase:advantages": "cross-env NODE_ENV=development ts-node scripts/supabase-db-manager.ts advantages",
    "backup:status": "cross-env NODE_ENV=production ts-node scripts/production-backup-manager.ts status",
    "backup:create": "cross-env NODE_ENV=production ts-node scripts/production-backup-manager.ts backup",
    "backup:recovery": "cross-env NODE_ENV=production ts-node scripts/production-backup-manager.ts recovery",
    "backup:advantages": "cross-env NODE_ENV=production ts-node scripts/production-backup-manager.ts advantages"
  },
  "keywords": [
    "hotel",
    "management",
    "api",
    "nodejs",
    "express"
  ],
  "author": "AirVikBook Team",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.862.0",
    "@aws-sdk/s3-request-presigner": "^3.862.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.1",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.2.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "sharp": "^0.34.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.7",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^6.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
